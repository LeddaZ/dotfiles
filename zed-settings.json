{
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "Google Sans Code",
  "buffer_font_size": 14.0,
  "gutter": {
    "min_line_number_digits": 2
  },
  "icon_theme": "Catppuccin Mocha",
  "indent_guides": {
    "line_width": 2
  },
  "terminal": {
    "font_family": "CommitMono Nerd Font Mono"
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_menus": true
  },
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "ui_font_family": "Google Sans Flex",
  "ui_font_features": {
    "ss02": 1
  }
}
